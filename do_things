Models:
Massage
  - id: integer
  - name: string
  - description: text
  - price: decimal
  - duration_in_minutes: integer
  - short_description: text
  - image: image

MessageReservation
  - id: integer
  - massage_id: foreign key - One massage has many reservations
  - masseur_id: foreign key - One masseur has many reservations
  - hour: time
  - day: date
  - created_at: datetime
  - updated_at: datetime

Masseur
  - id: integer
  - name: string
  - description: text
  - image: image
  - created_at: datetime
  - updated_at: datetime

MessageStudios
  - id: integer
  - name: string
  - description: text
  - main_image: image
  - gallery: image